{
  "mcpServers": {
    "financial-reports": {
      "command": "uv",
      "args": [
        "--directory",
        "/path/to/financial-reports-mcp",
        "run",
        "server.py"
      ],
      "env": {
        "USE_MOCK_API": "True",
        "API_KEY": "your_api_key_here"
      }
    }
  }
}
