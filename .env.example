# Financial Reports MCP Configuration

# API Key for the Financial Reports API
# Required when USE_MOCK_API=False
API_KEY="your_api_key_here"

# Base URL for the Financial Reports API
# Default API endpoint - usually no need to change
API_BASE_URL="https://api.financialreports.eu/"

# Use mock API instead of making real API calls
# Set to True for development or testing without an API key
# Set to False to use the real API (requires valid API_KEY)
USE_MOCK_API=True

# Server host and port (used when running directly or in Docker)
MCP_HOST="0.0.0.0"
MCP_PORT=8000
